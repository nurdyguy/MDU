<script>
    $(document).ready(function ()
    {
        $('.showSlides').on('click', function ()
        {
            var carId = $(this).data('car-id');
            ShowSlideshow(carId);
        });
    });

    function ShowSlideshow(carId)
    {
        $('#waitingOverlay').show();
        $.ajax(
        {
            url: '/cars/slideshow/' + carId,
            type: "GET",
            success: function (data)
            {
                $('#carSlideshow').html(data).modal("show");

            },
            error: function (e)
            {

            },
            complete: function ()
            {
                $('#waitingOverlay').hide();
            }
        });

    }
    

</script>